  "compilerOptions": {
    "module": "commonjs",
    "target": "ESNext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "sourceMap": true,
    "noEmit": true,
    "declaration": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["ES6", "DOM"]
  },
  "exclude": ["node_modules"],
}
